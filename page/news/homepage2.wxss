.mkt-ad-ccc .cm-slide div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, textarea, p, blockquote, th, td, hr, button, article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section { margin: 0;padding: 0;}
.mkt-ad-ccc .cm-slide ul, ol {list-style: none;}
.mkt-ad-ccc fieldset, img, button, input {border: 0;}
.mkt-ad-ccc .cm-slide .cm-slide-list:before, .cm-slide .cm-slide-list:after{content: "";display: table}
.mkt-ad-ccc .cm-slide .cm-slide-list:after{clear: both}
.mkt-ad-ccc .cm-slide{overflow: hidden; position: relative;  height: 210px}
.mkt-ad-ccc .cm-slide .cm-slide-list{width: 4000px;  position: absolute}
.mkt-ad-ccc .cm-slide .cm-slide-list .cm-slide-item{position: relative;float: left}
.mkt-ad-ccc .cm-slide--full-img img {  display: block;width: 100%;height:100%;}
.mkt-ad-ccc .cui-navContainer{position: absolute;z-index: 10;text-align: center;left: auto!important;right: 3px;bottom: 3px;font-size: 0;}
.mkt-ad-ccc .cui-slide-nav-item{overflow: hidden;width: 3px;height: 3px; background: rgba(255, 255, 255, .3);display: inline-block;margin: 0 2.5px;border-radius: 10px;border: 1px solid rgba(5,15,28,.05);}
.mkt-ad-ccc .cui-slide-nav-item-current{background: #fff;border-color: rgba(5,15,28,.08);width: 15px;}

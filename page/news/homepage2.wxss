.coupons_box {
    position: relative;
    min-height: 150rpx;
    padding-top: 6rpx;
    z-index: 2;
    background-color: #efefef;
}
.coupons_title {
    float: left;
    font-size: 32rpx;
    color: #8B8E94;
}
.coupons_number {
    position: relative;
    float: right;
    padding-right: 26rpx;
    font-size: 32rpx;
    color: #20A8E1;
}
.coupons_arrow {
    position: absolute;
    top: 10rpx;
    right: 8rpx;
    width: 14rpx;
    height: 14rpx;
    font-size: 0;
    border-right: 5rpx solid #28ABE2;
    border-bottom: 5rpx solid #28ABE2;
    transform: rotate(-45deg);
    transform-origin: center center;
}
.coupons_list {
    display: flex;
    clear: both;
    width: 100%;
    padding-top: 15rpx;
    box-sizing: border-box;
}
.coupons_list .coupons_list-item {
    position: relative;    
    flex: 1 1 auto;
    height: 125rpx;
    margin-right:6px;
    padding: 10rpx 0 10rpx 215rpx;
    box-sizing: border-box;
    background-color: #fff;
    border-radius: 10rpx;
    box-shadow: 1rpx 10rpx 10rpx rgba(192,192,192,0.2);
}
.coupons_list .coupons_list-item:nth-child(2) {
    margin: 0;
}
.coupons_list .coupons_list-item::after,
.coupons_list .coupons_list-item::before {
    content: "";
    position: absolute;
    left: 200rpx;
    width: 16rpx;
    height: 16rpx;
    overflow: hidden;
    border-radius: 50%;
    background-color: #efefef;
}
.coupons_list .coupons_list-item::before {
    top: -6rpx;
}
.coupons_list .coupons_list-item::after {
    height: 8rpx;
    border-radius: 8rpx 8rpx 0 0;
    bottom: 0;
    box-shadow: 1rpx 10rpx 10rpx rgba(192,192,192,0.2) inset;
}
.coupons_detail {
    position: absolute;
    top: 10rpx;
    left: 0;
    box-sizing: border-box;
    width: 210rpx;
    padding-top: 6rpx;
    text-align: center;
    color: #FD5064;
    border-right: 1px dashed #ccc;
}
.coupons_detail .coupons_con {
    display: inline-block;
    font-size: 24rpx;
    padding: 5rpx 8rpx;
    border-radius: 8rpx;
    border: 1px solid #FD5064;
}
.coupons_detail .coupons_price {
    display: block;
    text-align: center;
    font-size: 48rpx;
}
.coupons_detail .coupons_unit {
    margin-left: 6rpx;
    font-size: 32rpx;
}
.coupons_type {
    width: 120rpx;
    height: 100%;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-align-items: center;
    align-items: center;
    font-size: 28rpx;
    color: #8B8E94;
    padding:0 10rpx;
    box-sizing:border-box; 
    overflow: hidden; 
}
.coupons_type text{
  display:inline-block;
  text-align: justify;
  max-height:84rpx;
  line-height: 1.5;
  overflow: hidden;
}
.coupons_list .coupons_list-item:only-child {
    margin-right: 0;
    padding-left: 275rpx;
}
.coupons_list .coupons_list-item:only-child::before,
.coupons_list .coupons_list-item:only-child::after {
    left: 260rpx;
}
.coupons_list .coupons_list-item:only-child .coupons_detail {
    width: 268rpx;
}
.coupons_list .coupons_list-item:only-child .coupons_type {
    width: 100%;
    box-sizing: border-box;
    padding: 0 15rpx 0 50rpx;
    -webkit-align-items: flex-start;
    align-items: flex-start;
}
.coupons_list .coupons_list-item:only-child .coupons_type text {
    font-size: 32rpx;
}
.coupons_list .coupons_list-item:only-child .coupons_type .date {
    padding-top: 10rpx;
    font-size: 24rpx;
    opacity: 0.5;
}
